{
	"name": "shramov/leaflet-plugins",
	"description": "Miscellaneous plugins for Leaflet library for services that need to display route information and need satellite imagery from different providers",
	"version": "1.1.0",
	"keywords": [
		"leaflet",
		"plugins",
		"map",
		"google",
		"bing",
		"yandex"
	],
	"scripts": [
		"control/Permalink.js",
		"control/Permalink.Marker.js",
		"control/Permalink.Line.js",
		"control/Permalink.Layer.js",
		"control/Distance.js",
		"control/Layers.Load.js",
		"layer/*.js",
		"layer/vector/GPX.js",
		"layer/vector/GPX.Speed.js",
		"layer/vector/KML.js",
		"layer/vector/OSM.js",
		"layer/tile/*.js"
	],
	"styles": [
		"css/*.css"
	],
	"files": [
		"control/*",
		"layer/*",
		"layer/vector/*",
		"layer/tile/*",
		"layer/*",
		"css/*"
    ]
}